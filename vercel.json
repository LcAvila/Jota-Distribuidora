{
  "version": 2,
  "name": "sistema-jota",
  "builds": [
    {
      "src": "Front-end/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "Front-end/$1"
    }
  ]
}
